# Environment Configuration
RTSP_URL=rtsp://your_camera_ip:554/stream
CLIP_DURATION=20
CLIP_OUTPUT_DIR=./recordings
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=gemma3n:e4b
OLLAMA_TEXT_MODEL=gemma3n:e4b
LOG_LEVEL=INFO
DETECTION_CONFIDENCE_THRESHOLD=0.5
ANALYSIS_PROMPT="Analyze this video for security concerns. Look for suspicious activities, potential burglary attempts, unusual behavior, or any security threats. Provide a detailed assessment."

# Mobile Notification Configuration
ENABLE_MOBILE_NOTIFICATIONS=true
NOTIFICATION_MIN_THREAT_LEVEL=MEDIUM
MOBILE_NOTIFICATION_URL=http://192.168.1.100:3000/notify
NOTIFICATION_WEBHOOK_URLS=http://192.168.1.100:8000/webhook,http://192.168.1.101:8000/webhook
NOTIFICATION_SERVER_PORT=8888
